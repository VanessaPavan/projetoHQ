<form action="/enviarFormulario" id="formulario_contato" method="post" role="form">
  <fieldset>
    <label for="nome">Nome:</label>
    <input id="nome" name="nomeContato" type="text" required="required">
    <br />
    <br />
    <label for="email">Email: </label>
    <input id="email" name="emailContato" type="text" required="required">
    <br />
    <br />
    <label for="mensagem">Mensagem:</label>
    <br />
    <br />
    <textarea id="mensagem" name="mensagem" rows="5" required="required"></textarea>
    <br />
    <br />
    <button type="submit">Enviar</button>
  </fieldset>
</form>